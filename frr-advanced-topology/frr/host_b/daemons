zebra=yes      # Main routing daemon (required)
bgpd=yes       # BGP daemon
ospfd=no       # OSPF daemon (not needed)
ospf6d=no      # OSPFv6 daemon (not needed)
ripd=no        # RIP daemon (not needed)
ripngd=no      # RIPng daemon (not needed)
isisd=no       # IS-IS daemon (not needed)
pimd=no        # PIM daemon (not needed)
ldpd=no        # LDP daemon (not needed)
nhrpd=no       # NHRP daemon (not needed)
eigrpd=no      # EIGRP daemon (not needed)
babeld=no      # BABEL daemon (not needed)
sharpd=no      # SHARP daemon (not needed)
pbrd=no        # PBR daemon (not needed)
bfdd=no        # BFD daemon (not needed)
fabricd=no     # Fabric daemon (not needed)
vrrpd=no       # VRRP daemon (not needed)
pathd=no       # Path daemon (not needed)
